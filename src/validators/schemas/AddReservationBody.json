{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "guestName": {
            "type": "string"
        },
        "meta": {
            "properties": {
                "endTime": {
                    "format": "date-time",
                    "type": "string"
                },
                "notes": {
                    "type": "string"
                },
                "personsToServe": {
                    "type": "number"
                },
                "startTime": {
                    "format": "date-time",
                    "type": "string"
                }
            },
            "required": [
                "endTime",
                "personsToServe",
                "startTime"
            ],
            "type": "object"
        },
        "tableId": {
            "type": "string"
        }
    },
    "required": [
        "guestName",
        "meta",
        "tableId"
    ],
    "type": "object"
}

